data = dengue_data,
family = poisson(link = "log"))  # Specify Poisson distribution
# Summarize the multivariate Poisson regression model
summary(mv_poisson)
# Reporting results
report(mv_poisson)
# table
mv_poisson |>
tbl_regression()
